<p id="notice"><%= notice %></p>

<h1>Search Twitter</h1>

<table id="table_id" class="display datatable">
  <thead>
    <tr>
      <th>Screen Name</th>
      <th>Body</th>
    </tr>
  </thead>

  <tbody>
    <% @tweets.collect do |tweet| %>
      <tr>
        <td><%= "#{tweet.user.screen_name}" %></td>
        <td><%= "#{tweet.text}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
